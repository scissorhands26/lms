    "updateRule": null,
    "deleteRule": null,
    "options": {
      "query": "WITH student_exam_data AS (\nSELECT\n    (ROW_NUMBER() OVER()) as id,\n    (SELECT GROUP_CONCAT(value) \n         FROM (SELECT value FROM json_each(question_bank.correct_options) ORDER BY value)\n    ) as json1,\n    (SELECT GROUP_CONCAT(value) \n         FROM (SELECT value FROM json_each(exam_answers.answer) ORDER BY value)\n    ) as json2,\n    exam_answers.user AS user,\n    exam_answers.exam AS exam,\n    exam_answers.attempt AS attempt,\n    exam_answers.answer AS answer,\n    question_bank.question AS question,\n    question_bank.options AS options,\n    question_bank.correct_options AS correct_options,\n    exams.name as exam_name,\n    users.first_name as user_first_name,\n    users.last_name as user_last_name,\n    users.rank as user_rank\nFROM exam_attempts \nJOIN exam_answers \nON exam_attempts.id = exam_answers.attempt\nJOIN question_bank\nON exam_answers.question = question_bank.id\nJOIN exams\nON exam_attempts.exam = exams.id\nJOIN users\nON exam_attempts.user = users.id\n)\nSELECT\n    student_exam_data.id,\n    users.id as user_id,\n    exam_attempts.exam as exam_id,\n    exam_attempts.id as exam_attempt_id,\n    user_rank,\n    user_first_name,\n    user_last_name,\n    exam_name,\n    answer,\n    CAST(json1 = json2 AS BOOL) AS correct,\n    question,\n    options,\n    correct_options\nFROM\n    student_exam_data\nJOIN exam_attempts ON attempt = exam_attempts.id\nJOIN users ON student_exam_data.user = users.id"
    }
